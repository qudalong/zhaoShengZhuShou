.info-list{
  padding:0 20rpx ;
  background: #fff;
}
.info-list .item{
 display: flex;
 justify-content: space-between;
 align-items: center;
 font-size: 32rpx;
 padding: 20rpx 0;
}
.info-list .item .title{
  width: 180rpx;
}

.info-list .item .input{
  flex: 1;
}
.info-list .item .title text{
 color: red;
 vertical-align: middle;
 padding-right: 10rpx;
 width: 20rpx;
 display: inline-block;
}
.mt10{
  margin-top: 20rpx;
}

