.table-title {
  margin-top: 20rpx;
  display: flex;
  background: #4ab848;
  color: #fff;
  padding: 10rpx 0;
  border-radius: 4px 4px 0 0;
}

.table-title view {
  flex: 1;
  text-align: center;
}
.mark{
  margin-bottom: 20rpx;
}
.table-td {
  display: flex;
  color: #000;
  height: 60rpx;
  line-height: 60rpx;
}

.table-td view {
  flex: 1;
  text-align: center;
  border-left: 1px solid #4ab848;
  border-bottom: 1px solid #4ab848;
}

.table-td view:last-of-type {
  border-right: 1px solid #4ab848;
}
.definition{
  color: #666;
}
.textarea{
  background: #f0f0f0;
 width: 98%;
  border: 1px solid #ddd;
  border-radius: 10rpx;
  margin: 10rpx auto 30rpx;
  padding: 1%;
}
.mbfiexd{
  margin-bottom: 120rpx;
}