/*步骤  */

.top-nav {
  margin: 30rpx 0;
  font-size: 32rpx;
  text-align: center;
}

.nav {
  margin: 40rpx 0 20rpx;
  padding: 0 50rpx;
}

.nav .line {
  height: 2rpx;
  width: 95%;
  background: #ccc;
  position: relative;
}

.active-line{
  height: 2rpx;
   position: absolute;
  top: 50%;
  left: 20rpx;
  transform: translateY(-50%);
  width:31%;
  background: #54bc52;
  z-index: 9;
}
.active-line2{
  width:64%;
}
.active-line3{
  width:100%;
}
.nav .line .circle {
  height: 20rpx;
  width: 20rpx;
  border-radius: 50%;
  background: #e2e3e5;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  z-index: 10;
}

.nav .line .active {
  background: #54bc52;
}

.one {
  left: 0;
}

.two {
  left: 34%;
}

.three {
  left: 67%;
}

.four {
  left: 100%;
}


.nav-text {
  width: 94%;
  margin: 0 auto;
  display: flex;
  font-size: 24rpx;
  margin-top: 10rpx;
  justify-content: space-between;
}

.info-wrap {
  width: 84%;
  margin: 0 auto;
  background: #fff;
  border-radius: 5px;
  padding: 30rpx;
  box-shadow: 0 0 4px #ccc;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.info-wrap .item text {
  color: red;
  vertical-align: middle;
}

.item-wrap {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}

.item-wrap .item {
  height: 80rpx;
  width: 147rpx;
  line-height: 80rpx;
  text-align: center;
  background: #f0f0f0;
  border-radius: 5px;
  margin: 40rpx 0 0;
  margin-right: 10rpx;
  font-size: 26rpx;
}
